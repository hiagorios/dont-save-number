{
  "expo": {
    "name": "Dont save the number",
    "slug": "dont-save-number",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/image/icon.png",
    "splash": {
      "image": "./src/assets/image/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "src/assets/*",
      "**/*"
    ],
    "ios": {
      "supportsTablet": false
    },
    "android": {
      "package": "br.com.hriosdev.dontsavenumber",
      "versionCode": 1,
      "permissions" : [

      ],
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/image/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./src/assets/image/favicon.png"
    }
  }
}
